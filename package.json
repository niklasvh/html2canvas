    "lint": "eslint src/**",
    "test": "npm run flow && npm run lint && npm run test:node && npm run karma",
    "test:node": "mocha tests/node/*.js",
    "karma": "node karma",
    "watch": "webpack --progress --colors --watch",
    "start": "node tests/server"
